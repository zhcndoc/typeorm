    "eslint-plugin-jsdoc": "^62.7.0",
    "globals": "^17.3.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.1.0",
    "gulp-replace": "^1.1.4",
    "gulp-shell": "^0.8.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulpclass": "^0.2.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.2.7",
    "mocha": "^11.7.5",
    "mongodb": "^6.21.0",
    "mssql": "^12.2.0",
    "mysql2": "^3.17.4",
    "oracledb": "^6.10.0",
    "pg": "^8.18.0",
    "pg-query-stream": "^4.12.0",
    "pkg-pr-new": "^0.0.63",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-toml": "^2.0.6",
    "redis": "^5.11.0",
    "sinon": "^21.0.1",
    "sinon-chai": "^4.0.1",
    "sort-package-json": "^3.6.1",
    "source-map-support": "^0.5.21",
    "sql.js": "^1.14.0",
